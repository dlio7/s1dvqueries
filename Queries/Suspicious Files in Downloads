( TgtFileExtension In Anycase  ( "iso"    , "js"    , "hta"    , "html"    , "wsf"    , "lnk"    , "htm" ) AND TgtFilePath In Contains Anycase  ( "invoice"      , "password"      , "order"      , "payment"      , "receipt"      , "ready"      , "purchase"         , "inv"      , "music"     , "ticket"      , "docs"      , "statement"    , "messageatt" ) ) AND TgtFilePath In Contains Anycase  ( "\Downloads" ) AND NOT EndpointMachineType = "Server" AND SrcProcImagePath In Contains Anycase  ( "chrome"    , "edge"    , "opera"    , "safari"    , "brave"    , "firefox"    , "iexplore" )
