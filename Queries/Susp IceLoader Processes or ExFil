( ( SrcProcName Contains Anycase "xcopy" AND CmdLine Contains Anycase ".dat" ) OR SrcProcCmdLine Contains Anycase "A B C D E F" OR ( SrcProcName In Anycase  ( "powershell.exe" ) AND SrcProcCmdLine Contains Anycase ".lnk" ) OR ( SrcProcName Contains Anycase "rundll32" AND SrcProcCmdLine Contains ".dat" AND NOT SrcProcCmdLine In Contains Anycase  ( "ProgramData"      , "www.dat" ) ) OR ( SrcProcName Contains Anycase "rclone" AND SrcProcCmdLine Contains Anycase "-q" ) )
